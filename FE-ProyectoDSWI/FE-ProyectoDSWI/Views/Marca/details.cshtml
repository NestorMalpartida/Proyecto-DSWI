@model FE_ProyectoDSWI.Models.MarcaDetalle

@{
    ViewData["Title"] = "details";
}

<h1>Detalle</h1>

<div>
    <h4>Marca</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            CÓDIGO : 
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.codigo)
        </dd>
        <dt class = "col-sm-2">
            DESCRIPCIÓN : 
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.descripcion)
        </dd>
        <dt class = "col-sm-2">
            PAÍS : 
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.pais)
        </dd>
        <dt class = "col-sm-2">
            RUC : 
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.ruc)
        </dd>
        <dt class = "col-sm-2">
            TELÉFONO : 
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.telefono)
        </dd>
        <dt class = "col-sm-2">
            CORREO : 
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.correo)
        </dd>
    </dl>
</div>
<div>
    @Html.ActionLink("Actualizar", "update", new { codigo = Model.codigo }, new { @class = "btn btn-info" })
    @Html.ActionLink("Atrás", "findAll", new { }, new { @class = "btn btn-danger" })
</div>
