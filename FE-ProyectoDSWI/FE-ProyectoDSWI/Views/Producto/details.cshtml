@model FE_ProyectoDSWI.Models.ProductoDetalle

@{
    ViewData["Title"] = "details";
}

<h1>Detalles</h1>

<div>
    <h4>Auto</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            CÓDIGO : 
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.codigo)
        </dd>
        <dt class = "col-sm-2">
            NOMBRE : 
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.nombre)
        </dd>
        <dt class = "col-sm-2">
            DESCRIPCION : 
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.descripcion)
        </dd>
        <dt class = "col-sm-2">
            MARCA : 
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.marca)
        </dd>
        <dt class = "col-sm-2">
            PRECIO : 
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.precio)
        </dd>
    </dl>
</div>
<div>
    @Html.ActionLink("Actualizar", "update", new { codigo = Model.codigo }, new { @class = "btn btn-info" })
    @Html.ActionLink("Atrás", "findAll", new { }, new { @class = "btn btn-danger" })
</div>
