@model FE_ProyectoDSWI.Models.ClienteDetalle

@{
    ViewData["Title"] = "details";
}

<h1>Detalle</h1>

<div>
    <h4>Cliente</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            <label class="form-label">Código : </label>
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.codigo)
        </dd>
        <dt class = "col-sm-2">
            <label class="form-label">Usuario : </label>
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.usuario)
        </dd>
        <dt class = "col-sm-2">
            <label class="form-label">Distrito : </label>
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.distrito)
        </dd>
        <dt class = "col-sm-2">
            <label class="form-label">Dirección : </label>
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.direccion)
        </dd>
    </dl>
</div>
<div>
    @Html.ActionLink("Actualizar", "update", new { codigo = Model.codigo }, new { @class = "btn btn-info" }) 
    @Html.ActionLink("Atrás", "findAll", new { }, new { @class = "btn btn-danger" })
</div>
